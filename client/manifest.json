{
  "manifest_version": 3,
  "name": "PhishEye",
  "version": "1.0",
  "permissions": ["activeTab", "scripting", "storage", "notifications"],
  "icons": {
    "96": "logo.jpg"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },

  "browser_specific_settings": {
    "gecko": {
      "id": "phisheye@example.com"
    }
  }
}
