{
  "manifest_version": 3,
  "name": "PhishEye",
  "version": "1.0",
	"permissions": ["activeTab", "scripting", "storage", "notifications"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
